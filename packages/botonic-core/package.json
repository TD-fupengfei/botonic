{
  "name": "@botonic/core",
  "version": "0.18.2",
  "license": "MIT",
  "description": "Build Chatbots using React",
  "main": "src/index.js",
  "scripts": {
    "test": "../../node_modules/.bin/jest --coverage",
    "prepare": "node ../../preinstall.js",
    "lint": "date && echo xxxx && pwd && touch babel.config.js && ../../node_modules/.bin/eslint_d --fix --cache --quiet '.*.js' '*.js' 'src/**/*.js*' 'src/**/*.d.ts'",
    "lintx": "npm run lint_core -- --fix && ../../node_modules/.bin/tsc",
    "lint_ts": "../../node_modules/.bin/tsc --incremental false --skipLibCheck false",
    "lint_ci": "npm run lint_core && npm run lint_ts",
    "lint_core": "../../node_modules/.bin/eslint_d --cache --quiet '.*.js' '*.js' 'src/**/*.js*' 'src/**/*.d.ts'",
    "build": "rm -rf lib && babel src --out-dir lib --source-maps --copy-files",
    "cloc": "../../scripts/qa/cloc-package.sh ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubtype/botonic.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/hubtype/botonic/issues"
  },
  "files": [
    "src/**",
    "lib/**",
    "src/index.d.ts"
  ],
  "dependencies": {
    "axios": "^0.21.0",
    "pusher-js": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-plugin-add-module-exports": "^1.0.2"
  },
  "homepage": "https://github.com/hubtype/botonic#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "bot-framework",
    "chatbot",
    "conversational-app",
    "conversational-ui",
    "javascript"
  ]
}
